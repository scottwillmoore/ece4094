{
	"$schema": "https://json.schemastore.org/prettierrc.json",
	"endOfLine": "auto",
	"importOrder": [
		"^@ece4094/.*$",
		"^@/.*$",
		"^[./](?!.*\\.s?css$).*$",
		"^[./].*\\.module\\.s?css$",
		"^[./].*\\.s?css$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"overrides": [
		{
			"files": "*.html",
			"options": {
				"singleAttributePerLine": false
			}
		}
	],
	"printWidth": 100,
	"singleAttributePerLine": true,
	"useTabs": true
}
